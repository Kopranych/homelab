          "datasource": {
            "type": "influxdb",
            "uid": "influxdb-homelab"
          },
          "query": "import \"strings\"\n\ncpu = from(bucket: \"metrics\")\n  |> range(start: -1m)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"docker_container_cpu\")\n  |> filter(fn: (r) => r[\"_field\"] == \"usage_percent\")\n  |> last()\n  |> group(columns: [\"container_name\"])\n  |> keep(columns: [\"container_name\", \"_value\"])\n  |> rename(columns: {_value: \"cpu_percent\"})\n\nmem = from(bucket: \"metrics\")\n  |> range(start: -1m)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"docker_container_mem\")\n  |> filter(fn: (r) => r[\"_field\"] == \"usage\")\n  |> last()\n  |> group(columns: [\"container_name\"])\n  |> keep(columns: [\"container_name\", \"_value\"])\n  |> rename(columns: {_value: \"memory_bytes\"})\n\nstatus = from(bucket: \"metrics\")\n  |> range(start: -1m)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"docker_container_status\")\n  |> filter(fn: (r) => r[\"_field\"] == \"uptime_ns\")\n  |> last()\n  |> group(columns: [\"container_name\"])\n  |> keep(columns: [\"container_name\", \"container_status\"])\n\njoin(\n  tables: {cpu: cpu, mem: mem, status: status},\n  on: [\"container_name\"]\n)",
          "refId": "A"
        }
      ],
